{
  "name": "graasp-app-excalidraw",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "author": "Graasp",
  "contributors": [
    "Luna Godier"
  ],
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@excalidraw/excalidraw": "^0.14.2",
    "@graasp/apps-query-client": "^0.4.2",
    "@graasp/sdk": "0.9.2",
    "@mui/icons-material": "5.11.11",
    "@mui/lab": "5.0.0-alpha.123",
    "@mui/material": "5.11.13",
    "@sentry/react": "7.43.0",
    "@sentry/tracing": "7.43.0",
    "@testing-library/react": "13.4.0",
    "@types/lodash.debounce": "4.0.7",
    "i18next": "22.4.12",
    "immutable": "4.1.0",
    "lodash.debounce": "4.0.8",
    "lodash.isobject": "3.0.2",
    "lodash.isstring": "4.0.1",
    "qs": "6.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "react-scripts": "5.0.1",
    "react-toastify": "9.1.1",
    "typescript": "4.9.5",
    "uuid": "9.0.0"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.local react-scripts --max_old_space_size=4096 -r @cypress/instrument-cra start",
    "start:dev": "env-cmd -f ./.env.development react-scripts --max_old_space_size=4096 -r @cypress/instrument-cra start",
    "start:test": "env-cmd -f ./.env.test react-scripts --max_old_space_size=4096 -r @cypress/instrument-cra start",
    "start:ci": "react-scripts --max_old_space_size=4096 -r @cypress/instrument-cra start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test:cra": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "prettier:write": "prettier {src,cypress}/**/*.{ts,tsx,js,jsx} --write",
    "prettier:check": "prettier {src,cypress}/**/*.{ts,tsx,js,jsx} --check",
    "hooks:install": "husky install",
    "hooks:uninstall": "husky uninstall",
    "pre-commit": "yarn prettier:check && yarn lint",
    "lint": "eslint .",
    "cypress:open": "env-cmd -f ./.env.development cypress open",
    "test": "concurrently -k -s first \"yarn start:test\" \"wait-on http://127.0.0.1:3333 && yarn test:ci\" ",
    "test:ci": "env-cmd -f ./.env.test cypress run --browser chrome --headless && nyc report --reporter=text --reporter=text-summary",
    "cov:report": "open ./coverage/lcov-report/index.html",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/i18n": "0.13.6",
    "@types/lodash.isobject": "3.0.7",
    "@types/lodash.isstring": "4.0.7",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "concurrently": "7.6.0",
    "cypress": "12.8.1",
    "env-cmd": "10.1.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "nyc": "15.1.0",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "standard-version": "9.5.0",
    "wait-on": "7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/config/sentry.ts",
      "src/data/db.ts"
    ]
  },
  "packageManager": "yarn@3.5.0"
}
